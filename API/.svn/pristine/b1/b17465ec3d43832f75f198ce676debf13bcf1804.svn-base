using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace WebApiBia.Common.Assert
{
    public class AssertUtil
    {
        public string AssertIsNullOrEmptyReturnZero(string value)
        {
            if (String.IsNullOrEmpty(value))
            {
                value = "0";
            }
            return value;
        }
    }
}
