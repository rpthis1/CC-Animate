using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace WebApiBia.Common.Assert
{
    public class AssertPropertyValues
    {
        public string AssertStringProperty(dynamic prop)
        {
            if (prop != null)
            {
                return prop.GetMergedString();

            }
            return null;
        }
  
    }
}
